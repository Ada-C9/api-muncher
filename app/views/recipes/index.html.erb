<section class="muncher-home">
  <h1>MUNCH THIS</h1>
  <% if @recipes %>
    <ul>
      <% @recipes.each do |item| %>
        <li>
          <img src="<%= item["recipe"]["image"] %>" >
          <%= item["recipe"]["label"] %>
          by <%= item["recipe"]["source"] %>
        </li>
        <% end %>
    </ul>
    <% end %>
</section>
