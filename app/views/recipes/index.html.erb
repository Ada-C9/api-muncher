<h1>Recipes#index</h1>
<p>Find me in app/views/recipes/index.html.erb</p>

<section class="recipes-list">
  <h1>Recipe Search Result</h1>
  <% if @recipes %>
    <% raise %>
    <ul>
      <% @recipes.each do |recipe| %>
        <li>
          <% recipe.name %>
          <img src= "#{recipe.image}" alt= "#{recipe.name}" >
        </li>
      <% end %>
    </ul>
  <% end %>
</section>
