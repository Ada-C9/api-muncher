<h1>Recipes#index</h1>
<p>Find me in app/views/recipes/index.html.erb</p>
<%= render partial: "form" %>
<h2> Search Results </h2>

<section>

  <% @recipes.each do |recipe| %>
  <p>  <%= link_to image_tag(recipe.image, alt: recipe.label), recipe_path(recipe.uri_trim) %> </p>
  <p><%= recipe.label %></p>
  <% end %>
